<%= simple_form_for(@session, :html => { :class => 'form-horizontal' }) do |f| %>
  <fieldset>
    <legend><%= controller.action_name.capitalize %> Session</legend>
  <%= f.input :interviewer %>
  <%= f.input :company %>
  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
  </div>
  </fieldset>

<% end %>